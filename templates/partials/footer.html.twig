{% block analytics %}
  {% if theme_config.google_analytics_code %}
    {% include 'partials/analytics.html.twig' %}
  {% endif %}
{% endblock %}

{% block javascripts %}
  {% do assets.addJs('jquery', 101) %}
  {% do assets.addJs('theme://js/custom.js', 100) %}
  {% do assets.addJs('theme://js/modernizr.custom.71422.js', 100) %}
  
  {{ assets.js() }}

  {% if config.plugins.simplesearch.enabled %}
    <script src="/grav-docs/user/plugins/simplesearch/js/simplesearch.js" type="text/javascript" ></script>
  {% endif %}
{% endblock %}